<h1>BikeReady</h1>
<%= button_to "Sign Up", new_user_path %>

<%= form_for @user, url: login_path do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password_digest %><br>
    <%= f.text_field :password_digest %>
  </p>

  <p>
    <%= f.submit 'log-in' %>
  </p>
<% end %>
